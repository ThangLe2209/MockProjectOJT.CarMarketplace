{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "RabbitMq": {
    "Host": "localhost",
    "Username": "guest",
    "Password": "guest",
    "SharedSecret": "your-very-secret-value"
  },
  "AllowedOrigins": [
    "http://localhost:3000"
  ],
  "ReverseProxy": {
    "Routes": {
      "auth": {
        "ClusterId": "authCluster",
        "Match": { "Path": "/api/Authentication/{**catch-all}" }
      },
      "car": {
        "ClusterId": "carCluster",
        "Match": { "Path": "/api/Cars/{**catch-all}" }
      },
      "order": {
        "ClusterId": "orderCluster",
        "Match": { "Path": "/api/Orders/{**catch-all}" }
      }
    },
    "Clusters": {
      "authCluster": {
        "Destinations": {
          "authApi": { "Address": "http://localhost:5168/" }
        }
      },
      "carCluster": {
        "Destinations": {
          "carApi": { "Address": "http://localhost:5001/" }
        }
      },
      "orderCluster": {
        "Destinations": {
          "orderApi": { "Address": "http://localhost:5030/" }
        }
      }
    }
  }
}
